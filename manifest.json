{
    "manifest_version": 3,
    "name": "Google Maps Crawler",
    "version": "1.0",
    "description": "Extension để crawl dữ liệu từ Google Maps",
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "downloads"
    ],
    "host_permissions": [
        "https://www.google.com/maps/*",
        "https://maps.google.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Google Maps Crawler"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/maps/*",
                "https://maps.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/logo.jpg",
        "48": "icons/logo.jpg",
        "128": "icons/logo.jpg"
    }
}